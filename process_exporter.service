[Unit]
Description=Process Exporter Service
After=network.target

[Service]
User=nodeuser
Group=nodeuser
Type=simple
ExecStart=/usr/local/bin/process-exporter --config.path=/etc/process_exporter/config.yml --web.listen-address="0.0.0.0:9256"
Restart=on-failure

[Install]
WantedBy=multi-user.target

